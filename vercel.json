{
    "version": 2,
    "builds": [
        {
            "src": "main.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/socket.io/(.*)",
            "dest": "/main.py"
        },
        {
            "src": "/(.*)",
            "dest": "/main.py"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1"
    }
}